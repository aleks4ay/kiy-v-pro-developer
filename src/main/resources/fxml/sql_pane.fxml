<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="600.0" prefWidth="1400.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.aleks4ay.developer.controller.ControllerSimpleSql">
  <children>
        <VBox layoutX="14.0" layoutY="14.0" prefHeight="280.0" prefWidth="618.0" AnchorPane.bottomAnchor="110.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
           <children>
              <Label text="SQL:">
                 <VBox.margin>
                    <Insets left="8.0" />
                 </VBox.margin>
              </Label>
              <TextField fx:id="sql" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="966.0">
                 <VBox.margin>
                    <Insets left="6.0" />
                 </VBox.margin>
              </TextField>
            <ScrollPane VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="6.0" right="6.0" />
               </VBox.margin>
               <content>
                    <Label fx:id="result" prefHeight="430.0" prefWidth="1350.0" text="Result" />
               </content>
            </ScrollPane>
           </children>
        </VBox>
        <Button fx:id="select" layoutX="14.0" layoutY="512.0" mnemonicParsing="false" onAction="#goSql" text="Выбрать данные" />
        <Button fx:id="update" layoutX="165.0" layoutY="512.0" mnemonicParsing="false" onAction="#goSql" text="Вставить данные" />
  </children>
</AnchorPane>
